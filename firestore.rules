rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    
    // Warning: These rules are insecure and are meant for development only.
    // They allow public read and write access to your entire database.
    // Before deploying to production, you must secure your rules.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
